<!-- Modal ModalAddUpdateUser-->
<div class="modal fade" style="padding-top:3%!important" id="ModalAddUpdateEntidades" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                @*<h3 class="m-0 font-weight-bold text-primary" id="ModalCenterTitle"><span class="fas fa-user-o fa-lg" aria-hidden="true"></span> Registro de Entidad</h3>*@
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <!-- Overflow Hidden -->
                <form class="needs-validation form-group" id="formAddUpdateEntidades" autocomplete="off" registration novalidate>
                    <div class="card mb-2">
                        <div class="card-header py-3 text-center">
                            <h3 id="ModalCenterTitleH6" class="m-0 font-weight-bold text-primary"></h3>
                        </div>

                        <div class="card-body">
                            <div class="form-group">
                                <div class="input-group col-md-12">
                                    <input type="text" class="form-control" id="IdEntidadHidden" hidden>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group col-md-12">
                                    <input type="text" class="form-control" id="IdTipoEntidadHidden" hidden>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-2 mb-3">
                                    <label class="my-1 mr-2" for="lblClave">Clave:</label>
                                    <input type="text" class="form-control" id="IdInputClaveEntidad" placeholder="Clave">
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label class="my-1 mr-2" for="lblDescripcion">Descripción:</label>
                                    <input type="text" class="form-control" id="IdinputDescripcionEntidad" name="IdinputDescripcionEntidad" placeholder="Descripción">
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label for="lblSiglas">Siglas</label>
                                    <input type="text" class="form-control" id="IdInputSiglasEntidad" name="IdInputSiglasEntidad" placeholder="Siglas">
                                    <div class="invalid-feedback">
                                        Please provide a valid city.
                                    </div>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-5 mb-3">
                                    <label for="lblSelectedTipo">Tipo</label>
                                    <select class="form-select" id="IdSelectedTipo" name="IdSelectedTipo"></select>
                                    <div class="invalid-feedback">
                                        Please provide a valid city.
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    @*<div class="border-top my-3 border-5"></div>*@
                    <!-- Overflow Hidden -->

                </form>
            </div>
            <div class="modal-footer">
                <button class="btn btn-primary float-right" onclick="AddUpdateEntidades();" type="submit" value="submit">Aceptar</button>
                <button class="btn btn-danger float-right" onclick="CloseModalAddUpdateEntidades();">Cancelar</button>
            </div>
        </div>
    </div>
</div>


